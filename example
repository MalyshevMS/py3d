from py3d import *

vol = Volume(10, 10, 10) # creating 10x10x10 Volume
box = Box(vol, 0, 0, 0, 1, 1, 1, "box1", True) # creating 1x1x1 Cube at x: 0, y: 0, z: 0
box.display("xy", "@") # displaying cube:

# %---------------------%
# | @ . . . . . . . . . |
# | . . . . . . . . . . |
# | . . . . . . . . . . |
# | . . . . . . . . . . |
# | . . . . . . . . . . |
# | . . . . . . . . . . |
